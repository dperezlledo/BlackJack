package com.iescomercio.utilidades;

/**
 *
 * @author dapelle
 */
public class Aleatorio {
    private int max, min;

    public Aleatorio(int max, int min) {
        this.max = max;
        this.min = min;
    }
    
    public int nextAleatorio() {
        return (int)(Math.random()*(max-min+1)+min); 
    }
    
    
}
